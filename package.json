{
    "name": "in-time",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "antd": "^3.16.4",
        "electron-serve": "^0.2.1",
        "local-storage": "^2.0.0",
        "lodash": "^4.17.11",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.0.0",
        "react-scripts": "2.1.8"
    },
    "main": "main.js",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
